        {
          "action": "addStrategy(bytes32,(address,address,bool)[])",
          "args": {},
          "executer": "user1",
          "expect": "fail",
          "message": "caller is not the operator"
        },
        {
          "action": "addStrategies(bytes32[],(address,address,bool)[][])",
          "args": {},
          "executer": "user1",
          "expect": "fail",
          "message": "caller is not the operator"
